<script type="text/javascript">
 //IDをキーにする場合と異なり、getElementByTagNameメソッドを使ってタグ名をキーに要素を取り出す。
 //タグ名では複数の要素が合致する可能性がある。
 //あるページに含まれるアンカータグを全て取得し、そのリンク先を一覧表示する
 window.onload = function(){
     //リンクリストを格納する配列
     var result = [];
     //ページ内の全てのアンカータグを取得
     var list = document.getElementByTagName('a');
     //リスト(NodeListオブジェクト)から順にアンカータグを取り出し、そのhref属性を配列resultに格納
     //item(i):i番目のノードを取得
     for(var i = 0; i < list.length; i++){
         result.push(list.item(i).href);
     }
     //配列の内容を改行区切りで連結し、ダイアログ表示
     window.alert(result.join('\n'));
 }
</script>

