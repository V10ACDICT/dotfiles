use strict;
use warnings;

my %last_name = (
    'fred' => 'fliintstone',
    'dino' => undef,
    'barney'=>'rubble',
    'betty'=>'rubble',
);
print keys %last_name;

print keys %last_name;

my @k = keys %last_name;
print @k;


while( (my $key, my $value) = each %last_name){
    print "$key => $value\n";
}
