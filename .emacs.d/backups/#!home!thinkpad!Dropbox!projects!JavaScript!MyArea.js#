//コンストラクタ(base,heightプロパティの定義)
var MyArea = function(base, height){
    this.base = base;
    this.height = heiht;
};
//getTriangle, getSquareメソッドの定義
MyArea.prototype = {
    getTriagle : function(){return this.base * this.height / 2;},
    getSquare : function(){return this.base * this.height;}
};
